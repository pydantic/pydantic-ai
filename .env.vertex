# Vertex AI test configuration
# These variables are auto-filled by run-vertex-tests.sh from gcloud config.
# You can also set them manually here if needed.

# Unset Google API keys to force Vertex auth path
unset GOOGLE_API_KEY GEMINI_API_KEY

# Project ID - auto-filled from: gcloud config get-value project
export GOOGLE_PROJECT=
export GOOGLE_CLOUD_PROJECT=

# Location - defaults to 'global', change if needed
# See: https://cloud.google.com/vertex-ai/docs/general/locations
export GOOGLE_LOCATION="global"
