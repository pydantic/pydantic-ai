{
  "name": "run-code",
  "version": "0.0.0",
  "main": "run.mjs",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "format": "prettier --write -- .",
    "lint": "eslint --max-warnings=0 src && prettier --check -- . && tsc --noEmit",
    "build": "tsc && node inline_python.cjs",
    "start-http": "node dist/httpServer.js",
    "start-stdio": "node dist/stdioServer.js",
    "dev-http": "npm run build && npm run start-http",
    "dev-stdio": "npm run build && npm run start-stdio"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "all",
    "tabWidth": 2,
    "printWidth": 119,
    "bracketSpacing": true
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.7.0",
    "pyodide": "^0.27.2",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^22.13.10",
    "eslint": "^9.22.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.1"
  }
}
