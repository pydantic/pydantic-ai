{
  "name": "mcp-run-python",
  "version": "0.0.0",
  "main": "dist/cli.js",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "format": "prettier --write -- .",
    "lint": "eslint --max-warnings=0 src && prettier --check -- . && tsc --noEmit",
    "build": "rm -rf dist && tsc && node inline_python.cjs",
    "sse": "node dist/cli.js sse",
    "stdio": "node dist/cli.js stdio",
    "dev-sse": "npm run build && npm run sse",
    "dev-stdio": "npm run build && npm run stdio"
  },
  "bin": {
    "mcp-run-python": "./dist/cli.js"
  },
  "files": ["dist"],
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "all",
    "tabWidth": 2,
    "printWidth": 119,
    "bracketSpacing": true
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.7.0",
    "pyodide": "^0.27.2",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^22.13.10",
    "eslint": "^9.22.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.1"
  }
}
