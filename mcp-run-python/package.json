{
  "name": "run-code",
  "version": "0.0.0",
  "main": "run.mjs",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "format": "prettier --write -- .",
    "lint": "eslint --max-warnings=0 src/*/src && prettier --check -- .",
    "start-http": "node --experimental-wasm-stack-switching dist/httpServer.js",
    "start-stdio": "node --experimental-wasm-stack-switching dist/stdioServer.js",
    "build": "tsc && node inline_python.cjs"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "all",
    "tabWidth": 2,
    "printWidth": 119,
    "bracketSpacing": true
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.7.0",
    "pyodide": "^0.27.2",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@types/express": "^5.0.0",
    "@types/node": "^22.13.10",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2"
  }
}
